# Copy this file to .env and fill in your actual values
# NEVER commit .env to git!

# Didit API (https://business.didit.me)
DIDIT_API_KEY=your_didit_api_key_here
DIDIT_APP_ID=your_didit_app_id_here
DIDIT_KYC_WORKFLOW_ID=your_kyc_workflow_id_here
DIDIT_LIVENESS_WORKFLOW_ID=your_liveness_workflow_id_here

# Gridlines API (https://gridlines.io) - for Voter ID verification
GRIDLINES_API_KEY=your_gridlines_api_key_here

# Surepass API (https://surepass.io) - backup for Voter ID
SUREPASS_API_KEY=your_surepass_api_key_here

# Webhook URL (your backend server)
WEBHOOK_BASE_URL=https://your-backend.com/api
